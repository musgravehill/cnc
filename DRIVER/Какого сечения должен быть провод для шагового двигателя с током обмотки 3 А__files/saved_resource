function searchInGoogle()
{
    var forum_path = '';

    

    var query = encodeURIComponent(YAHOO.util.Dom.getElementsByClassName("textbox", "input",fetch_object('navbar_search'))[0].value);
    window.open("http://www.google.ru/search?q=" + "site:forum.rcdesign.ru" + forum_path + "+" + query);
}
